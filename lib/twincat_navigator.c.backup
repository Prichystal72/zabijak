                GetWindowRect(subChild, &rect);

                int itemCount = SendMessage(subChild, LB_GETCOUNT, 0, 0);
                int height = rect.bottom - rect.top;
                int width = rect.right - rect.left;

                int score = itemCount + height / 10;
                
                // NEJLEVƒöJ≈†√ç ListBox m√° nejvy≈°≈°√≠ prioritu
                int relativeLeft = rect.left - parentRect.left;
                if (relativeLeft < leftmostPosition) {
