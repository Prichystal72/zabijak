            } else if (relativeLeft == leftmostPosition) {
                score += 500; // Stejn√° pozice = stejn√Ω bonus
            }
            // Ostatn√≠ ListBoxy nedost√°vaj√≠ poziƒçn√≠ bonus

            printf("[ListBox %d] Handle: 0x%p\n", listboxCount, (void*)childWindow);
            printf("  Text: '%s'\n", windowText);
            printf("  Pozice: (%d,%d) relativni: (%d,%d)\n", 
                   rect.left, rect.top, 
                   rect.left - parentRect.left, rect.top - parentRect.top);
            printf("  Velikost: %dx%d\n", width, height);
